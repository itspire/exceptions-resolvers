{
    "name": "itspire/exceptions-resolvers",
    "description": "Itspire Exceptions - Resolvers",
    "type": "library",
    "keywords": ["itspire", "exception", "resolver", "response"],
    "homepage": "http://www.itspire.fr",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Ryan RAJKOMAR",
            "email": "ryan.rajkomar@itspire.fr"
        }
    ],
    "require": {
        "php": ">=7.4",
        "itspire/exceptions-adapters": "~1.0",
        "itspire/exceptions-mappers": "~1.0",
        "nyholm/psr7": "~1.3",
        "psr/http-message": "~1.0",
        "symfony/translation": "~4.4"
    },
    "autoload": {
        "psr-4": {
            "Itspire\\Exception\\Resolver\\": "src/main/php"
        }
    },
    "require-dev": {
        "pdepend/pdepend": "~2.8",
        "phploc/phploc": "~7.0",
        "roave/security-advisories": "dev-latest",
        "sebastian/phpcpd": "~6.0",
        "squizlabs/php_codesniffer": "~3.5",
        "symfony/finder": "~4.4",
        "symfony/phpunit-bridge": "~5.1",
        "symfony/yaml": "~4.4"
    },
    "autoload-dev": {
        "psr-4": {
            "Itspire\\Exception\\Resolver\\Test\\": "src/test/php"
        }
    }
}
